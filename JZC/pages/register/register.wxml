<view class="container">
  
  <view class="content">
    <image class="background" src="data:{{image1.filetype}};base64,{{image1.data}}" alt="{{image1.alt_text}}"></image>
    <view class="SCU">SCU Makers</view>
    <image class="dial" src="data:image/svg+xml;base64,"></image>
    <button class="button" bindtap="handleRegister">注  册</button>
    <button class="code" bindtap="getVerifyCode">获取验证码</button>
    <view class="svg"></view>
    <view class="register-title">注册 SCU Makers 小程序</view>
    <view class="register-content">
      <form>
        <view class="form-item">
          <label>邮箱</label>
          <input class="preinput {{isFocused1 ? 'focused' : ''}}" type="text" placeholder="你的邮箱" value="{{email}}" bindfocus="handleFocus1" bindinput="handleEmailInput"
    bindblur="handleBlur1"/>
        </view>
        <view class="form-item">
          <label>验证码</label>
          <input class="preinput {{isFocused2 ? 'focused' : ''}}" type="text" placeholder="验证码" value="{{verifyCode}}"
      bindinput="handleVerifyCodeInput" bindfocus="handleFocus2"
    bindblur="handleBlur2"/>
        </view>
        <view class="form-item" bindtap="handleContainerClick">
          <label>密码</label>
          <input
          class="preinput {{isFocused3 ? 'focused' : ''}}"
           type="{{isPasswordVisible1 ? 'text' : 'password'}}"
           placeholder="密码"
           value="{{password}}"
           bindinput="handlePasswordInput"
           bindfocus="handleFocus3"
           bindblur="handleBlur3"
           focus="{{isFocus1}}"
          />
          <image
          src="{{isPasswordVisible1 ? 'data:image/svg+xml;base64,' : 'data:image/svg+xml;base64,'}}"
          catchtap="togglePasswordVisibility1"
          data-visible="{{isPasswordVisible1}}"
          class="password-toggle-icon"
          />
        </view>
        <view class="form-item" bindtap="handleContainerClick">
          <label>确认密码</label>
          <input
          class="preinput {{isFocused4 ? 'focused' : ''}}"
           type="{{isPasswordVisible2 ? 'text' : 'password'}}"
           placeholder="确认密码"
           value="{{confirmPassword}}"
           bindinput="handleConfirmPasswordInput"
           bindfocus="handleFocus4"
           bindblur="handleBlur4"
           focus="{{isFocus2}}"
          />
          <image
            src="{{isPasswordVisible2 ? 'data:image/svg+xml;base64,' : 'data:image/svg+xml;base64,'}}"
            catchtap="togglePasswordVisibility2"
            data-visible="{{isPasswordVisible2}}"
            class="password-toggle-icon"
          />
        </view>
      </form>
      <text class="login-link" bindtap="navigateToRegister">返回登录界面</text>
    </view>
  </view>
</view>